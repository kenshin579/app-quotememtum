{
  "manifest_version": 2,
  "name": "Quotememtum",
  "description": "Chrome Extension for Quotememtum",
  "version": "1.0.0",
  "icons": {
    "16": "./assets/images/favicons/favicon-16x16.png",
    "128": "./assets/images/favicons/quote-128.png"
  },
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "browser_action": {
    "default_icon": "./assets/images/favicons/favicon-16x16.png"
  },
  "content_security_policy": "script-src 'self' https://quote.advenoh.pe.kr; object-src 'self'",
  "permissions": [
    "storage",
    "tabs"
  ]
}
